<form method="post" action="{{ path('app_veterinaire_habitat_delete', {'id': habitat.id}) }}" onsubmit="return confirm('Êtes-vous sûr de vouloir supprimer cet élément?');">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ habitat.id) }}">
    <button class="btn btn-outline-light my-3">Supprimer</button>
</form>
